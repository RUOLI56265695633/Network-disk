{"ast":null,"code":"export default {\n  name: \"Login\",\n  data() {\n    return {\n      dialogVisible: true,\n      form: {\n        role: 'ADMIN'\n      },\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入账号',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      },\n      imgs: [require('@/assets/imgs/1.jpg'), require('@/assets/imgs/2.jpg'), require('@/assets/imgs/3.jpg')],\n      slideVerifyShow: false\n    };\n  },\n  created() {},\n  methods: {\n    onSuccess() {\n      this.$request.post('/login', this.form).then(res => {\n        if (res.code === '200') {\n          localStorage.setItem(\"xm-user\", JSON.stringify(res.data)); // 存储用户数据\n          this.$message.success('登录成功');\n          setTimeout(() => {\n            if (res.data.role === 'ADMIN') {\n              location.href = '/home';\n            } else {\n              location.href = '/front/home?category=all';\n            }\n          }, 500);\n        } else {\n          this.$message.error(res.msg);\n          this.slideVerifyShow = false;\n        }\n      });\n    },\n    login() {\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          this.slideVerifyShow = true;\n          // 验证通过\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","dialogVisible","form","role","rules","username","required","message","trigger","password","imgs","require","slideVerifyShow","created","methods","onSuccess","$request","post","then","res","code","localStorage","setItem","JSON","stringify","$message","success","setTimeout","location","href","error","msg","login","$refs","validate","valid"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div style=\"height: 60px; position: fixed; top: 0; display: flex; align-items: center; padding-left: 20px\">\n      <img src=\"@/assets/imgs/logo.png\" alt=\"\" style=\"width: 50px\">\n      <span style=\"color: #409EFF; font-size: 24px; font-weight: bold; margin-left: 5px\">个人网盘系统</span>\n    </div>\n\n<!--    <div style=\"flex: 1; display: flex; justify-content: center\">-->\n<!--      <img src=\"@/assets/imgs/login-bg.jpg\" alt=\"\" style=\"width: 80%\">-->\n<!--    </div>-->\n\n    <div style=\"flex: 1; display: flex; justify-content: center\">\n      <div style=\"width: 350px; position: relative; padding: 50px 30px; box-shadow: 0 0 10px rgba(0,0,0,.2); background-color: white; border-radius: 15px;\">\n        <div style=\"position: absolute; background-color: white; top: 100px; z-index: 999\" v-if=\"slideVerifyShow\">\n          <slide-verify :l=\"42\"\n                        :r=\"10\"\n                        :w=\"300\"\n                        :h=\"155\"\n                        :accuracy=\"5\"\n                        :imgs=\"imgs\"\n                        slider-text=\"向右滑动\"\n                        @success=\"onSuccess\"\n                        ref=\"slideVerifyRef\"\n          ></slide-verify>\n        </div>\n        <div style=\"text-align: center; font-size: 24px; font-weight: bold; margin-bottom: 30px; color: #333\">欢 迎 登 录 网 盘 系 统</div>\n        <el-form :model=\"form\" :rules=\"rules\" ref=\"formRef\">\n          <el-form-item prop=\"username\">\n            <el-input size=\"medium\" prefix-icon=\"el-icon-user\" placeholder=\"请输入账号\" v-model=\"form.username\"></el-input>\n          </el-form-item>\n          <el-form-item prop=\"password\">\n            <el-input size=\"medium\" prefix-icon=\"el-icon-lock\" placeholder=\"请输入密码\" show-password  v-model=\"form.password\"></el-input>\n          </el-form-item>\n          <el-form-item prop=\"role\">\n            <el-select style=\"width: 100%\" size=\"medium\" v-model=\"form.role\">\n              <el-option value=\"ADMIN\" label=\"管理员\"></el-option>\n              <el-option value=\"USER\" label=\"用户\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item>\n            <el-button round type=\"primary\" size=\"medium\" style=\"width: 100%; color: white\" @click=\"login\">登 录</el-button>\n          </el-form-item>\n          <div style=\"display: flex; align-items: center\">\n            <div style=\"flex: 1\"></div>\n            <div style=\"flex: 1; text-align: right\">\n              没有账号？请先 <a href=\"/register\">注册</a>\n            </div>\n          </div>\n        </el-form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      dialogVisible: true,\n      form: { role: 'ADMIN' },\n      rules: {\n        username: [\n          { required: true, message: '请输入账号', trigger: 'blur' },\n        ],\n        password: [\n          { required: true, message: '请输入密码', trigger: 'blur' },\n        ]\n      },\n      imgs: [\n          require('@/assets/imgs/1.jpg'),\n          require('@/assets/imgs/2.jpg'),\n          require('@/assets/imgs/3.jpg'),\n      ],\n      slideVerifyShow: false\n    }\n  },\n  created() {\n\n  },\n  methods: {\n    onSuccess() {\n      this.$request.post('/login', this.form).then(res => {\n        if (res.code === '200') {\n          localStorage.setItem(\"xm-user\", JSON.stringify(res.data))  // 存储用户数据\n          this.$message.success('登录成功')\n          setTimeout(() => {\n            if (res.data.role === 'ADMIN') {\n              location.href = '/home'\n            } else {\n              location.href = '/front/home?category=all'\n            }\n          }, 500)\n        } else {\n          this.$message.error(res.msg)\n          this.slideVerifyShow = false\n        }\n      })\n    },\n    login() {\n      this.$refs['formRef'].validate((valid) => {\n        if (valid) {\n          this.slideVerifyShow = true\n          // 验证通过\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.container {\n  height: 100vh;\n  overflow: hidd